<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <FSharpCoreImplicitPackageVersion>6.0.*</FSharpCoreImplicitPackageVersion>
    <NoWarn>FS0020</NoWarn>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Include="StaticWebsiteApimApiPolicy.xml" />
    <None Include="StaticWebsiteApimGetOperationPolicy.xml" />
    <None Include="StaticWebsiteApimGetIndexOperationPolicy.xml" />
    <None Include="StaticWebsiteApimPostOperationPolicy.xml" />
    <None Include="APIApimApiPolicy.xml" />
    <None Include="Pulumi.yaml" />
    <None Include="Pulumi.msft.yaml" />
    <None Include="Pulumi.test.yaml" />
    
    <Compile Include="Async.fs" />
    <Compile Include="LetsEncrypt.fs" />
    <Compile Include="Output.fs" />
    <Compile Include="Certificate.fs" />
    <Compile Include="Program.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Update="FSharp.Core" Version="6.0.5" />
    <PackageReference Include="Certes" Version="3.0.3" />
    <PackageReference Include="DnsClient" Version="1.6.1" />
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles" Version="2.2.0" />
    <PackageReference Include="Pulumi.FSharp" Version="3.37.2" />
    <PackageReference Include="Pulumi.FSharp.AzureAD" Version="5.18.0.5" />
    <PackageReference Include="Pulumi.FSharp.AzureNative" Version="1.62.0.14" />
    <PackageReference Include="Pulumi.FSharp.Core" Version="3.0.2" />
    <PackageReference Include="Pulumi.FSharp.Random" Version="4.4.2.3" />
  </ItemGroup>

</Project>
